    {
      "cell_type": "code",
      "source": [
        "import os\n",
        "from azure.storage.blob import BlobServiceClient\n",
        "import streamlit as st\n",
        "\n",
        "# Configurações (substitua pelas suas credenciais)\n",
        "AZURE_STORAGE_CONNECTION_STRING = \"SUA_CHAVE_DE_CONEXAO_AQUI\"\n",
        "CONTAINER_NAME = \"SEU_CONTAINER_AQUI\"\n",
        "\n",
        "def upload_blob(file, file_name):\n",
        "    try:\n",
        "        blob_service_client = BlobServiceClient.from_connection_string(AZURE_STORAGE_CONNECTION_STRING)\n",
        "\n",
        "        blob_client = blob_service_client.get_blob_client(container=CONTAINER_NAME, blob=file_name)\n",
        "\n",
        "        blob_client.upload_blob(file, overwrite=True)\n",
        "\n",
        "        return blob_client.url\n",
        "    except Exception as ex:\n",
        "        st.error(f\"Erro ao enviar o arquivo para o Azure Blob Storage: {ex}\")\n",
        "        return None\n",
        "\n"
      ],
      "metadata": {
        "id": "QWvJbgG3N3AQ"
      },
